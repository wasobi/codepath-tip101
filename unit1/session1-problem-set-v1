# Problem 3: Lunch Menu
def print_menu(menu):
	print("Lunch today is: " + menu)
print_menu("ğŸ•") # output: Lunch today is: ğŸ•

# Problem 4: Sum of Two Integers
def sum(a, b):
	return a + b
sum_of_integers = sum(13,27)
print(2*sum_of_integers)

# Problem 5: Product of Two Integers
def product(x,y):
	return x * y
print(product(22,7))
